{
    "version": 2,
    "builds": [
      { "src": "src/app/api/reviews/BackendReviewsAPI.py", "use": "@vercel/python" },
      { "src": "src/app/api/transactions/BackendTransaction.py", "use": "@vercel/python" },
      { "src": "src/app/api/Returns/BackendReturn.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/api/reviews", "dest": "src/app/api/reviews/BackendReviewsAPI.py" },
      { "src": "/api/transactions", "dest": "src/app/api/transactions/BackendTransaction.py" },
      { "src": "/api/return-car", "dest": "src/app/api/Returns/BackendReturn.py" }
    ]
  }
  